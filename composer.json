{
    "name": "finpar/backend-module",
    "description": "Backend module",
    "type": "laravel-module",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "Hermann D. Schimpf",
            "email": "hschimpf@hds-solutions.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "ddzobov/laravel-pivot-softdeletes": "^2.1",
        "hds-solutions/laravel-modules": "dev-develop",
        "spatie/laravel-permission": "^4.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "HDSSolutions\\Finpar\\": "src/",

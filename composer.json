        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "HDSSolutions\\Finpar\\BackendModuleServiceProvider"
            ],
            "aliases": {
                "Backend": "HDSSolutions\\Finpar\\Backend\\Facade"

{
    "name": "hds-solutions/backend-module",
    "description": "Backend module",
    "type": "laravel-module",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "Hermann D. Schimpf",
            "email": "hschimpf@hds-solutions.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "ajcastro/eager-load-pivot-relations": "^0.2",
        "hds-solutions/laravel-modules": "dev-develop",
        "spatie/laravel-permission": "^4.0",
        "staudenmeir/eloquent-has-many-deep": "^1.13",
        "yajra/laravel-datatables": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "HDSSolutions\\Laravel\\": "app/",
            "HDSSolutions\\Laravel\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "HDSSolutions\\Laravel\\BackendModuleServiceProvider"
            ],
            "aliases": {
                "Backend": "HDSSolutions\\Laravel\\Backend\\Facade"
